<h1>this is the user's <%= title %></h1>
<section id="feed-post">
  <h4>Posts</h4>
  <% if(locals.user) {%>
  <form action="/posts/create" id="new-post-form" method="post">
    <textarea
      name="content"
      id=""
      cols="30"
      rows="3"
      placeholder="type here"
      required
    ></textarea>
    <input
      type="submit"
      value="Post
    "
    />
  </form>
  <%}%>
  <div id="post-list-container">
    <ul>
      <% for(post of posts) { %> <%-include('_post')-%> <%}%>
    </ul>
  </div>
</section>
